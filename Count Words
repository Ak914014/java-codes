import java.util.Scanner;

public class CountWords {
    
    public static void CountW( String str){
      
        int n = str.length();
        int temp = 0;
        for (int i = 0; i < n; i++) {
            
            if(str.charAt(i)== ' '){
                
                temp++;

            }
        }
            System.out.println(temp);

    }
    public static void main(String [] args){

        Scanner sc = new Scanner(System.in);
        String Str = sc.nextLine(); 
        CountW(Str);
    }
}
