public class AllSubstrings{

    public static void SubString(String str){

       
        for (int i = 0; i < str.length(); i++) {
             String str1 = "";
            for (int j = i; j < str.length(); j++) {
                 
                str1 = str1+str.charAt(j);
                System.out.println(str1);

            }

        }
    }
    public static void main(String[] args) {
        String str = "abc";
        SubString(str);
    }
}
