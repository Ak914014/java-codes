import java.util.Scanner;

public class check {

	public static void checkprime(int n){

		boolean prime = true;
		for( int i = 2; i<n; i++){
              
			if(n%i==0){
				prime = false;
			
				break;
			}
			else{
				prime = true;
				
			}

			
		}
		System.out.println(prime);
	}
    public static void main(String[] args) {
    
    	Scanner sc = new Scanner(System.in);
		int n =sc.nextInt();
		checkprime(n);
		
	}
}
